/***/ "./js/src/light-or-dark.js":
/*!*********************************!*\
  !*** ./js/src/light-or-dark.js ***!
  \*********************************/
/*! exports provided: lightOrDark */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
eval("__webpack_require__.r(__webpack_exports__);\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"lightOrDark\", function() { return lightOrDark; });\n// //main.js\n// import {myVar} from './module';\n// import {functionName} from './module';\n//\n// functionName();\n// console.log(myVar);\n//\n// //module.js\n// export const myVar = \"xxx\";\n//\n// export function functionName() {\n//   console.log('yyy')\n// }\nfunction lightOrDark(color, return_bool = true) {\n  // Variables for red, green, blue values\n  var r, g, b, hsp; // Check the format of the color, HEX or RGB?\n\n  if (color.match(/^rgb/)) {\n    // If RGB --> store the red, green, blue values in separate variables\n    color = color.match(/^rgba?\\((\\d+),\\s*(\\d+),\\s*(\\d+)(?:,\\s*(\\d+(?:\\.\\d+)?))?\\)$/);\n    r = color[1];\n    g = color[2];\n    b = color[3];\n  } else {\n    // If hex --> Convert it to RGB: http://gist.github.com/983661\n    color = +(\"0x\" + color.slice(1).replace(color.length < 5 && /./g, '$&$&'));\n    r = color >> 16;\n    g = color >> 8 & 255;\n    b = color & 255;\n  } // HSP (Highly Sensitive Poo) equation from http://alienryderflex.com/hsp.html\n\n\n  hsp = Math.sqrt(0.299 * (r * r) + 0.587 * (g * g) + 0.114 * (b * b)); // Using the HSP value, determine whether the color is light or dark\n  // console.log(color,hsp)\n\n  if (return_bool) {\n    if (hsp > 128) {\n      return 'light';\n    } else {\n      return 'dark';\n    }\n  } else {\n    return hsp;\n  }\n}\n;//# sourceURL=[module]\n//# sourceMappingURL=data:application/json;charset=utf-8;base64,\n//# sourceURL=webpack-internal:///./js/src/light-or-dark.js\n");

/***/ }),

